syn on
set background=dark
set filetype=on
set incsearch
set ignorecase
set number
set hlsearch
set smartcase
set scrolloff=2
set tabstop=4
set shiftwidth=4
set foldmethod=indent
set foldlevelstart=20
set nocompatible
set ruler
set previewheight=1
set smartindent
set showmatch
set backspace=indent,eol
"set list

nmap <F3> :tabp<CR>
nmap <F4> :tabn<CR>
